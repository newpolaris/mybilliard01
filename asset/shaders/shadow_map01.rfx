float4 ps_main( float1 inDepth: TEXCOORD0 ) : COLOR0
{
    float4 Depth;
    Depth.r = floor( inDepth[ 0 ] * 127.f ) / 127.f;
    Depth.g = floor( ( inDepth[ 0 ] - Depth.r ) * 127.f * 127.f ) / 127.f;
    Depth.b = 0.f;
    Depth.a = 1.f;
    return Depth;
}
]]></RmShaderCode></RmHLSLShader>
