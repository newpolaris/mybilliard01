shared float4x4 Light0_ViewProjection;
float depthBias;


struct VS_INPUT 
{
   float4 Position : POSITION0;   
};

struct VS_OUTPUT 
{
   float4 Position :        POSITION0;
   float1 Depth :           TEXCOORD0;   
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   float4 pos      = mul( Input.Position, Light0_ViewProjection );
   pos.z          += pos.w * depthBias;

   Output.Position = pos;

   Output.Depth    = pos.z / pos.w;

   return( Output );
}

]]></RmShaderCode>
